<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BrowserStack-Grid" thread-count="3" parallel="tests" verbose="3">
  
  <test name="CromeTest">
  <parameter name="browserName" value="Chrome"></parameter>
  <parameter name="browserVersion" value="104.0"></parameter>
  <parameter name="os" value="Windows"></parameter>
  <parameter name="osVersion" value="11"></parameter>
    <classes>
      <class name="com.tests.RegisterPageTest"/>
    </classes>
  </test>
  
  <test name="FirefoxTest">
  <parameter name="browserName" value="Firefox"></parameter>
  <parameter name="browserVersion" value="103.0"></parameter>
  <parameter name="os" value="Windows"></parameter>
  <parameter name="osVersion" value="10"></parameter>
    <classes>
      <class name="com.tests.RegisterPageTest"/>
    </classes>
  </test>
  
  <test name="EdgeTest">
  <parameter name="browserName" value="Edge"></parameter>
  <parameter name="browserVersion" value="104.0"></parameter>
  <parameter name="os" value="OS X"></parameter>
  <parameter name="osVersion" value="Catalina"></parameter>
    <classes>
      <class name="com.tests.RegisterPageTest"/>
    </classes>
  </test>
</suite> 
